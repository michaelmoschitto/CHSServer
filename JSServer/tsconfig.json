{
   "compilerOptions": {
       "module": "commonjs",
       "esModuleInterop": true,
       "target": "es6",
       "noImplicitAny": true,
       "noImplicitThis": true,
       "allowJs": true,
       "moduleResolution": "node",
       "sourceMap": false,
       "baseUrl": ".",
       "outDir": "built",
       "paths": {
           "*": [
               "node_modules/*"
           ]
       }
   },
   "include": [
       "Src/**/*"
   ]
}
